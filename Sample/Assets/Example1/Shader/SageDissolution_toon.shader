					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float2 uv_DisolveGuide = IN.ase_texcoord3.xy * _DisolveGuide_ST.xy + _DisolveGuide_ST.zw;
				float temp_output_88_0 = (-4.0 + (( (-0.6 + (( 1.0 - _DissolveAmount ) - 0.0) * (0.6 - -0.6) / (1.0 - 0.0)) + tex2D( _DisolveGuide, uv_DisolveGuide ).r ) - 0.0) * (4.0 - -4.0) / (1.0 - 0.0));
				
				float3 Color = _StrokeColor.rgb;
				float Alpha = temp_output_88_0;
